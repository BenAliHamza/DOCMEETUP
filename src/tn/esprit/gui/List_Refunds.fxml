<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.gui.List_RefundsController">
    <children>
        <Button fx:id="listpurchases" layoutX="426.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleListPurchases" prefHeight="25.0" prefWidth="144.0" text="Check My Purchases" />
        <Button fx:id="insuranceprofile" layoutX="38.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleInsuranceProfile2" text="Return to My Insurance Profile" />
        <Button layoutX="401.0" layoutY="29.0" mnemonicParsing="false" text="My DocMeet-Up HomePage" />
        <TableView fx:id="refundTable" layoutX="38.0" layoutY="100.0" prefHeight="249.0" prefWidth="529.0">
            <columns>
                <TableColumn fx:id="userIdColumn" minWidth="0.0" prefWidth="0.0" text="User ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="userId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundIdColumn" minWidth="0.0" prefWidth="82.99993515014648" text="Refund ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="insuranceProfileIdColumn" minWidth="0.0" prefWidth="0.0" text="Insurance Profile ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="insuranceProfileId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundAmountColumn" prefWidth="108.9999771118164" text="Refund Amount">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundAmount" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundStatusColumn" prefWidth="173.99998474121094" text="Refund Status">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundStatus" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundMethodColumn" prefWidth="172.66671752929688" text="Refund Method">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundMethod" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundCommentsColumn" minWidth="0.0" prefWidth="101.66668701171875" text="Refund Comments">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundComments" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="purchaseIdColumn" minWidth="5.6666259765625" prefWidth="5.6666259765625" text="Purchase ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="purchaseId" />
                    </cellValueFactory>
                </TableColumn>
                
            </columns>
        </TableView>
      <Button fx:id="Refresh" layoutX="429.0" layoutY="66.0" mnemonicParsing="false" onAction="#Refresh" text="Refresh My Refund List" />
      <Text fontSmoothingType="LCD" layoutX="269.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="My Refunds" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
