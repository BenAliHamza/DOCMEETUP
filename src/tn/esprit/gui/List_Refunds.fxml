<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.gui.List_RefundsController">
    <children>
        <Button fx:id="listpurchases" layoutX="349.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleListPurchases" prefHeight="25.0" prefWidth="99.0" text="My Purchases" />
        <Button fx:id="insuranceprofile" layoutX="194.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleInsuranceProfile" text="My Insurance Profile" />
        <Button layoutX="399.0" layoutY="8.0" mnemonicParsing="false" text="My DocMeet-Up App" />
        <Button layoutX="545.0" layoutY="8.0" mnemonicParsing="false" text="Help" />
        <TableView fx:id="refundTable" layoutX="38.0" layoutY="100.0" prefHeight="249.0" prefWidth="529.0">
            <columns>
                <TableColumn fx:id="refundIdColumn" text="Refund ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="userIdColumn" text="User ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="userId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="insuranceProfileIdColumn" text="Insurance Profile ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="insuranceProfileId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundAmountColumn" text="Refund Amount">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundAmount" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundStatusColumn" text="Refund Status">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundStatus" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundMethodColumn" text="Refund Method">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundMethod" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="refundCommentsColumn" text="Refund Comments">
                    <cellValueFactory>
                        <PropertyValueFactory property="refundComments" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="purchaseIdColumn" text="Purchase ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="purchaseId" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </children>
</AnchorPane>
