<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.gui.consultations.controllers.FollowDocIController">
      <children>
      <VBox layoutX="14.0" layoutY="31.0" prefHeight="254.0" prefWidth="203.0" spacing="18.0">
         <children>
            <TextField fx:id="filterField" onKeyTyped="#search" promptText="search" />
            <TableView fx:id="tvpatient" onMouseClicked="#row" prefHeight="131.0" prefWidth="203.0">
              <columns>
                <TableColumn fx:id="tvfirname" prefWidth="101.0" text="First name" />
                <TableColumn fx:id="tvlasname" prefWidth="101.0" text="Last name" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <TableView fx:id="tvfollowup" layoutX="236.0" layoutY="31.0" prefHeight="100.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="tvdate" prefWidth="72.0" text="Date" />
          <TableColumn fx:id="tvbp" prefWidth="97.0" text="Blood pressure" />
            <TableColumn fx:id="tvheart" prefWidth="101.0" text="Heart Rate" />
            <TableColumn fx:id="tvtemp" prefWidth="104.0" text="Tempeture" />
            <TableColumn fx:id="tvweight" prefWidth="119.0" text="Weight" />
            <TableColumn fx:id="tvtemp1" prefWidth="104.0" text="Tempeture" />
        </columns>
      </TableView>
      <TableView fx:id="tvmedic" layoutX="236.0" layoutY="150.0" prefHeight="100.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="tvmedicname" prefWidth="243.0" text="Medication name" />
          <TableColumn fx:id="tvmedicdesc" prefWidth="256.0" text="Description" />
        </columns>
      </TableView>
      <TableView fx:id="tvanalyse" layoutX="236.0" layoutY="266.0" prefHeight="100.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="tvanalysename" prefWidth="225.0" text="Analyse Name" />
          <TableColumn fx:id="tvanalysedes" prefWidth="198.0" text="Analyse description" />
            <TableColumn fx:id="tvanalyseres" prefWidth="75.0" text="Result" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
