<AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="400.0" prefWidth="941.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.gui.consultations.controllers.DocTimeController">
