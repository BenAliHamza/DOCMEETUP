<columns>
                 <TableColumn fx:id="descriptiontv" prefWidth="263.0" text="description">
                     <cellValueFactory>
                         <PropertyValueFactory property="heure_debut" />
                     </cellValueFactory>
                 </TableColumn>
                 <TableColumn fx:id="statustv" prefWidth="161.0" text="status">
                     <cellValueFactory>
                         <PropertyValueFactory property="heure_fin" />
                     </cellValueFactory>
                 </TableColumn>
         <TableColumn fx:id="datetimetv" prefWidth="484.0" text="date and time">
            <cellValueFactory>
               <PropertyValueFactory property="heure_fin" />
            </cellValueFactory>
         </TableColumn>
             </columns>    </TableView>
   <Button fx:id="ValidS" layoutX="31.0" layoutY="340.0" mnemonicParsing="false" onAction="#validS" prefHeight="31.0" prefWidth="206.0" text="change status" />
   <Button fx:id="back" layoutX="740.0" layoutY="343.0" mnemonicParsing="false" onAction="#myap" prefHeight="31.0" prefWidth="186.0" text="return" />
